	   <!-- ################################################ Security property  ############################################## -->
		
	<stac:SecurityProperty rdf:about="Confidentiality">	
		<rdfs:label xml:lang="en">Confidentiality/Privacy </rdfs:label>
		<rdfs:comment xml:lang="en">Confidentiality means that only destined user must be able to read data. </rdfs:comment>
		<owl:sameAs rdf:resource="&herzog;Confidentiality"/>
		<owl:sameAs rdf:resource="&denker;Confidentiality"/>
		<owl:sameAs rdf:resource="&kim;Confidentiality"/>
		<owl:sameAs rdf:resource="&maso;Confidentialite"/>
		<owl:sameAs rdf:resource="&securitysw;Confidentiality"/>
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="Integrity">	
		<rdfs:label xml:lang="en">Integrity</rdfs:label>
		<rdfs:comment xml:lang="en">Data integrity guarantees that the transmitted messages are not modified by attackers, either due to malicious intent or by accident.</rdfs:comment>
		<dc:description xml:lang="en">Integrity means that no data is altered during the communication.</dc:description>
		<owl:sameAs rdf:resource="&herzog;Integrity"/>
		<owl:sameAs rdf:resource="&denker;DataIntegrity"/>
		<owl:sameAs rdf:resource="&kim;MessageIntegrity"/>
		<owl:sameAs rdf:resource="&maso;Integrite"/>
		<owl:sameAs rdf:resource="&securitysw;Integrity"/>
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="Authentication">	
		<rdfs:label xml:lang="en">Authentication</rdfs:label>
		<rdfs:comment xml:lang="en">Authentication prevents unauthorized access to the network.</rdfs:comment>
		<owl:sameAs rdf:resource="&herzog;Authentication"/>
		<owl:sameAs rdf:resource="&denker;Authentication"/> 
		<!-- <owl:sameAs rdf:resource="&kim;MessageIntegrity"/> UserAuthentication MessageAuthentication -->
		<owl:sameAs rdf:resource="&securitysw;Authentication"/>
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="NonRepudiation">	
		<rdfs:label xml:lang="en">Non Repudiation</rdfs:label>
		<rdfs:comment xml:lang="en">Non repudiation means that a sender is unable to deny having sent a message or infoomation.</rdfs:comment>
		<owl:sameAs rdf:resource="&herzog;Non-Repudiation"/>
		<owl:sameAs rdf:resource="&maso;Non-repudiation"/>
		<owl:sameAs rdf:resource="&securitysw;NonRepudiation"/>
	</stac:SecurityProperty>
		
	<stac:SecurityProperty rdf:about="Trust">	
		<rdfs:label xml:lang="en">Trust</rdfs:label>
		<rdfs:comment xml:lang="en">Data is reliable?</rdfs:comment>
		<dc:description xml:lang="en">Trust is defined as the degree to which a node should be trustworthy, secure, or reliable during any interaction with the node.</dc:description>       
		<owl:sameAs rdf:resource="&kim;Trust"/>
		<owl:sameAs rdf:resource="&maso;Confiance"/>
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="AccessControl">		
		<rdfs:label xml:lang="en">Access Control</rdfs:label>
		<rdfs:comment xml:lang="en">Access Control is the mechanism that allows owners of resources to define, manage and enforce access conditions applicable to each resource.</rdfs:comment>
		<owl:sameAs rdf:resource="&denker;AccessControl"/>
		<owl:sameAs rdf:resource="&securitysw;AccessControl"/>
		<owl:sameAs rdf:resource="&secberlin;AccessControl"/>	
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="Anonymity">	
		<rdfs:label xml:lang="en">Anonymity</rdfs:label>
		<rdfs:comment xml:lang="en">Anonymity protects user identity, making it hard to track the whereabouts of a certain user.</rdfs:comment>
		<dc:description xml:lang="en">Anonimity hides the source of a packet during communication.</dc:description>
		<owl:sameAs rdf:resource="&herzog;Anonymity"/>
		<owl:sameAs rdf:resource="&denker;Anonymity"/>
		<owl:sameAs rdf:resource="&kim;Anonymity"/>
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="Authorization">	
		<rdfs:label xml:lang="en">Authorization</rdfs:label>
		<rdfs:comment xml:lang="en">Only authorized persons can access to network services or resources.</rdfs:comment>
		<dc:description xml:lang="en"></dc:description>
		<owl:sameAs rdf:resource="&herzog;Authorisation"/>
		<owl:sameAs rdf:resource="&denker;Authorization"/>
		<owl:sameAs rdf:resource="&kim;Authorization"/>
		<owl:sameAs rdf:resource="&secberlin;Authorization"/>	
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="Accountability">	
		<rdfs:label xml:lang="en">Accountability</rdfs:label>
		<rdfs:comment xml:lang="en">Accountability is the requirement that actions of an entity may be traced uniquely to that entity.</rdfs:comment>
		<owl:sameAs rdf:resource="&herzog;Accountability"/>
		<owl:sameAs rdf:resource="&secberlin;Accountability"/>		
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="Availability">	
		<rdfs:label xml:lang="en">Availability</rdfs:label>
		<rdfs:comment xml:lang="en">Data availability ensures that the network is alive and that data are accessible.</rdfs:comment>
		<dc:description xml:lang="en">Availability ensures that services and information can be accessed at the time when they are required.</dc:description>
		<owl:sameAs rdf:resource="&herzog;Availability"/>
		<owl:sameAs rdf:resource="&kim;Availability"/>
		<owl:sameAs rdf:resource="&securitysw;Availability"/>
		<owl:sameAs rdf:resource="&maso;Disponibilite"/>	
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="DataFreshness">	
		<rdfs:label xml:lang="en">Data Freshness, Replay Protection</rdfs:label>
		<rdfs:comment xml:lang="en">Data freshness suggests that the data is recent, and it ensures that no old messages have been replayed.</rdfs:comment>
	</stac:SecurityProperty>
	
	<stac:SecurityProperty rdf:about="SemanticSecurity">	
		<rdfs:label xml:lang="en">Semantic Security</rdfs:label>
		<rdfs:comment xml:lang="en">Semantic security ensures that an eavesdropper has no information about the message, even if it sees multiple encryptions of the same messages.</rdfs:comment>
	</stac:SecurityProperty>
	

	<!-- ############################################################ CRYPTOGRAPHY ################################################################### -->
	
	<!-- stac:AsymmetricAlgorithm -->
	
	<stac:AsymmetricAlgorithm rdf:about="ECC">		
		<rdfs:label xml:lang="en">Elliptic Curve Cryptography (ECC)</rdfs:label>
		<rdfs:comment xml:lang="en">Elliptic curve cryptography (ECC) is an approach to public-key cryptography based on the algebraic structure of elliptic curves. Because the RSA algorithm is rather inefficient in its use of key length, ECC has been proposed.</rdfs:comment>
		<rdf:type rdf:resource="&stac;KeyManagement"/>	
		<owl:sameAs rdf:resource="&kimalgorithms;ECC"/>	
		<owl:sameAs rdf:resource="&maso;Elliptic_curve"/>
		<owl:sameAs rdf:resource="&secberlin;EllipticCurveCryptography"/>
		<stac:satisfies rdf:resource="Authentication"/>
		<stac:satisfies rdf:resource="Confidentiality"/>
		<rdf:type rdf:resource="&stac;ManetSecurityMechanism"/>
	</stac:AsymmetricAlgorithm>
	
	<stac:AsymmetricAlgorithm rdf:about="RSA">		
