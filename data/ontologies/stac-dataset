		<dcterms:hasPart rdf:resource="GSMMobileStation"/>
		<dcterms:hasPart rdf:resource="BaseStationSubsystem"/>
	</stac:GSMArchitectureComponent>	
		
	<stac:GSMArchitectureComponent rdf:about="GSMMobileStation">		
		<rdfs:label xml:lang="en">GSM Mobile Station (MS)</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<dcterms:hasPart rdf:resource="MobileEquipment"/>
		<dcterms:hasPart rdf:resource="SIM"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="MobileEquipment">		
		<rdfs:label xml:lang="en">Mobile Equipment (ME)</rdfs:label>
		<rdfs:comment xml:lang="en">Mobile Equipment (ME) is the mobile phone</rdfs:comment>
		<dcterms:hasPart rdf:resource="IMEI"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="SIM">		
		<rdfs:label xml:lang="en">Subscriber Identity Module (SIM)</rdfs:label>
		<rdfs:comment xml:lang="en">SIM cards provide the security mechanism for GSM. SIM cards are like credit cards and identify the user to the GSM network.</rdfs:comment>
		<dcterms:hasPart rdf:resource="A3"/>
		<dcterms:hasPart rdf:resource="A8"/>
		<dcterms:hasPart rdf:resource="IMSI"/>
		<dcterms:hasPart rdf:resource="MobileStationIsdnNumber"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="IMEI">		
		<rdfs:label xml:lang="en">International Mobile Equipment Identity (IMEI)</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="IMSI">		
		<rdfs:label xml:lang="en">International Mobile Subscriber Identity (IMSI)</rdfs:label>
		<rdfs:comment xml:lang="en">The IMSI is a unique identifier for a mobile subscriber in a GSM and UMTS network. It is stored in the SIM card of the mobile station.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="MobileStationIsdnNumber">		
		<rdfs:label xml:lang="en">Mobile Station ISDN Number</rdfs:label>
