	<!-- Base Station Subsystem (BSS) -->
	
	
	<stac:GSMArchitectureComponent rdf:about="BaseStationSubsystem">		
		<rdfs:label xml:lang="en">Base Station Subsystem (BSS)</rdfs:label>
		<rdfs:comment xml:lang="en">Base Station Subsystem (BSS) controls the radio link with the Mobile Station (MS).</rdfs:comment>
		<dcterms:hasPart rdf:resource="BaseTransceiverStation"/>
		<dcterms:hasPart rdf:resource="BaseStationController"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="BaseTransceiverStation">		
		<rdfs:label xml:lang="en">Base Transceiver Station (BTS)</rdfs:label>
		<rdfs:comment xml:lang="en">The BTS is the radio equipment that transmits and receives information over the air to provide communication with the mobile station.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="BaseStationController">		
		<rdfs:label xml:lang="en">Base Station Controller (BSC)</rdfs:label>
		<rdfs:comment xml:lang="en">The BSC is responsible for the switching between several BTSs, and for the switching of radio channels.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<!-- Network Subsystem (NSS) -->
	
	<stac:GSMArchitectureComponent rdf:about="NSS">		
		<rdfs:label xml:lang="en">Network Subsystem (NSS)</rdfs:label>
		<rdfs:comment xml:lang="en">The NSS provides switching services between GSM and external networks.</rdfs:comment>
		<dcterms:hasPart rdf:resource="MSC"/>
		<dcterms:hasPart rdf:resource="HLR"/>
		<dcterms:hasPart rdf:resource="VLR"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="MSC">		
		<rdfs:label xml:lang="en">Mobile services Switching Center (MSC)</rdfs:label>
		<rdfs:comment xml:lang="en">Mobile services Switching Center (MSC) performs the switching of calls between the mobile users (mobile and fixed) one. The MSC also handles the mobility management operations.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<!-- Operation Support Subsystem (OSS) -->
	
	<stac:GSMArchitectureComponent rdf:about="OSS">		
		<rdfs:label xml:lang="en">Operation Support Subsystem (OSS)</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<dcterms:hasPart rdf:resource="OMC"/>
		<dcterms:hasPart rdf:resource="EIR"/>
		<dcterms:hasPart rdf:resource="Auc"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="OMC">		
		<rdfs:label xml:lang="en">Operations and Maintenance Center (OMC)</rdfs:label>
		<rdfs:comment xml:lang="en">Operations and Maintenance Center (OMC) supervises the proper operation and setup of the network.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="EIR">		
		<rdfs:label xml:lang="en">Equipment Identity Register (EIR)</rdfs:label>
		<rdfs:comment xml:lang="en">The EIR contains a list of valid mobile equipment.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="Auc">		
		<rdfs:label xml:lang="en">Authentication Center (AuC)</rdfs:label>
		<rdfs:comment xml:lang="en">AuC includes information for identifying authorized users of the GSM or GPRS network and thus prevent unauthorized use of the network.</rdfs:comment>
		<dcterms:hasPart rdf:resource="A3"/>
		<dcterms:hasPart rdf:resource="A8"/>
	</stac:GSMArchitectureComponent>
	
	<!-- Interface GSM -->
	
	<stac:GSMArchitectureComponent rdf:about="UmInterface">		
		<rdfs:label xml:lang="en">Um Interface</rdfs:label>
		<rdfs:comment xml:lang="en">The Mobile Station and the BSS communicate across the Um interface, also known as the radio link or the air interface.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="AInterface">		
		<rdfs:label xml:lang="en">AInterface</rdfs:label>
