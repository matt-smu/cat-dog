	<stac:GSMArchitectureComponent rdf:about="HLR">		
		<rdfs:label xml:lang="en">Home Location Register (HLR)</rdfs:label>
		<rdfs:comment xml:lang="en">HLR is the database that holds subscription information for every person.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="VLR">		
		<rdfs:label xml:lang="en">Visitor Location Register (VLR)</rdfs:label>
		<rdfs:comment xml:lang="en">The VLR is a subscriber profile containing temporary information, and is distributed in the network according to geographical locations. The VLR performs the authentication and key agreement procedure.</rdfs:comment>
	</stac:GSMArchitectureComponent>
	
	<!-- Security GSM -->
	
	<stac:GSMSecurityAlgorithm rdf:about="COMP128">		
		<rdfs:label xml:lang="en">COMP128</rdfs:label>
		<rdfs:comment xml:lang="en">COMP128 is an implementation of the A3 and A8 algorithms defined in the GSM standard.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Malicious actions against the GPRS technology [Xenakis 2006]</dc:description>
		<stac:hasFeature rdf:resource="Deprecated"/>
	</stac:GSMSecurityAlgorithm>
	
	<stac:GSMSecurityAlgorithm rdf:about="A3">		
		<rdfs:label xml:lang="en">A3</rdfs:label>
		<rdfs:comment xml:lang="en">The A3 algorithm is the authentication algorithm for GSM networks. The A3 algorithm resides on the SIM card and at the AuC/HLR. The main purpose of this algorithm is to authenticate the identity of a mobile subscriber.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Malicious actions against the GPRS technology [Xenakis 2006]</dc:description>
		<stac:satisfies rdf:resource="Authentication"/> 
	</stac:GSMSecurityAlgorithm>
	
	<stac:GSMSecurityAlgorithm rdf:about="A5_3">		
		<rdfs:label xml:lang="en">A5</rdfs:label>
		<rdfs:comment xml:lang="en">A5 is an encryption algorithm to encrypt the communication between the mobile phone and the base station (BTS Base Station Transceiver).</rdfs:comment>
		<rdf:type rdf:resource="&stac;StreamCipher"/>
		<stac:satisfies rdf:resource="Confidentiality"/>
		<dcterms:hasPart rdf:resource="Kasumi"/>
	</stac:GSMSecurityAlgorithm>
	
	<stac:GSMSecurityAlgorithm rdf:about="A8">		
		<rdfs:label xml:lang="en">A8</rdfs:label>
		<rdfs:comment xml:lang="en">A8 algorithm is the ciphering key generation algorithm.</rdfs:comment>
		<rdf:type rdf:resource="&stac;GSMKeyManagement"/>
		<stac:satisfies rdf:resource="Authentication"/>
	</stac:GSMSecurityAlgorithm>
	
	<stac:GSMArchitectureComponent rdf:about="PIN">		
		<rdfs:label xml:lang="en">Personal Identification Number (PIN)</rdfs:label>
		<rdfs:comment xml:lang="en">PIN: a secret numeric password shared between a user and a system that can be used to authenticate the user to the system.</rdfs:comment>
		<stac:satisfies rdf:resource="Authentication"/>
	</stac:GSMArchitectureComponent>
	
	<stac:GSMArchitectureComponent rdf:about="TMSI">		
		<rdfs:label xml:lang="en">Temporary Mobile Subscriber Identity (TMSI)</rdfs:label>
		<rdfs:comment xml:lang="en">The TMSI is transmitted by the MS (Mobile Station). The purpose of this TMSI is to deny an intruder the possibility of gaining information on the resources used by the subscriber, preventing of tracing the user's location.</rdfs:comment>
		<stac:satisfies rdf:resource="Anonymity"/>
	</stac:GSMArchitectureComponent>
	
