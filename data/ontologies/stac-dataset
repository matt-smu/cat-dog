		<stac:isVulnerabilityOf rdf:resource="&stac;WimaxTechnology"/>
		<stac:isVulnerabilityOf rdf:resource="&stac;GSMTechnology"/>
		<stac:isVulnerabilityOf rdf:resource="&stac;GPRSTechnology"/>
		<stac:isVulnerabilityOf rdf:resource="&stac;EDGETechnology"/>
