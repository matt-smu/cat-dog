		<rdf:type rdf:resource="&stac;KeyManagement"/>	
		<rdf:type rdf:resource="&stac;DigitalSignature"/>		
		<owl:sameAs rdf:resource="&kimalgorithms;RSA"/>
		<owl:sameAs rdf:resource="&securitysw;RSAAlgorithm"/>	
		<owl:sameAs rdf:resource="&maso;RSA"/>		
		<stac:satisfies rdf:resource="Authentication"/>
		<stac:satisfies rdf:resource="Confidentiality"/>
