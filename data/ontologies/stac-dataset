	<!-- ################################################ OSI Model Layer ############################################################################-->
		
	<stac:OSIModelLayer rdf:about="PhysicalLayer">		
		 <rdfs:label xml:lang="en">Physical Layer</rdfs:label>
		 <rdfs:comment xml:lang="en">The physical layer defines the electrical and physical specifications of the data connection.</rdfs:comment>
	</stac:OSIModelLayer>
	
	<stac:OSIModelLayer rdf:about="LinkLayer">		
		<rdfs:label xml:lang="en">Link Layer</rdfs:label>
		<rdfs:comment xml:lang="en">The link layer is responsible for a reliable link between two nodes, by detecting and correcting errors that may occur in the physical layer.</rdfs:comment>
	</stac:OSIModelLayer>
	
	<stac:OSIModelLayer rdf:about="NetworkLayer">		
		<rdfs:label xml:lang="en">NetworkLayer</rdfs:label>
		<rdfs:comment xml:lang="en">The network layer handles the adressing and routing of the data.</rdfs:comment>
	</stac:OSIModelLayer>
	
	<stac:OSIModelLayer rdf:about="TransportLayer">		
		<rdfs:label xml:lang="en">Transport Layer</rdfs:label>
		<rdfs:comment xml:lang="en">The transport layer manages packetization of data, then the delivery of the packets, including checking for errors in the data once it arrives.</rdfs:comment>
	</stac:OSIModelLayer>
	
	<stac:OSIModelLayer rdf:about="SessionLayer">		
		<rdfs:label xml:lang="en">Session Layer</rdfs:label>
		<rdfs:comment xml:lang="en">The session layer controls the dialogues between computers.</rdfs:comment>
	</stac:OSIModelLayer>
	
	<stac:OSIModelLayer rdf:about="PresentationLayer">		
		<rdfs:label xml:lang="en">Presentation Layer</rdfs:label>
		<rdfs:comment xml:lang="en">The presentation layer is usually part of an operating system (OS) and converts incoming and outgoing data from one presentation format to another.</rdfs:comment>
	</stac:OSIModelLayer>
	
	<stac:OSIModelLayer rdf:about="ApplicationLayer">		
		<rdfs:label xml:lang="en">Application Layer</rdfs:label>
		<rdfs:comment xml:lang="en">The application layer is the layer closest to end users, which means bot the ISO application layer and the users interact directly with the software applications.</rdfs:comment>
	</stac:OSIModelLayer>
	
