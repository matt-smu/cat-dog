	<!-- Sensor attacks -->
	
	<stac:SensorAttack rdf:about="Tampering">		
		<rdfs:label xml:lang="en">Node tampering</rdfs:label>
		<rdfs:comment xml:lang="en">An attacker can tamper with nodes physically and interrogate and compromise them by extracting sensitive information.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<stac:occursInLayer rdf:resource="PhysicalLayer"/>
		<stac:threatens rdf:resource="Confidentiality"/>
	</stac:SensorAttack>

	<stac:SensorAttack rdf:about="Collision">		
		<rdfs:label xml:lang="en">Collision</rdfs:label>
		<rdfs:comment xml:lang="en">An attacker may intentionally violate the communication protocol and continually transmit messages in an attempt to generate collisions ans also deplete a sensor node by forcing too many retransmissions. When two nodes transmit packets of data on the same frequency channel, it can lead to discard of the affected data packet for re-transmission.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015], Paper: MILENAGE: An example of good UMTS Security [Massari 2009?]</dc:description>
        <stac:occursInLayer rdf:resource="LinkLayer"/>
		<rdf:type rdf:resource="&stac;UMTSAttack"/>
		<rdf:type rdf:resource="&stac;RfidAttack"/>	
		<stac:hasSecurityMechanism rdf:resource="ErrorCorrectingCode"/>
		<stac:hasSecurityMechanism rdf:resource="CollisionDetection"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="Unfairness">		
		<rdfs:label xml:lang="en">Unfairness</rdfs:label>
		<rdfs:comment xml:lang="en">Unfairness is a weaker form DoS that is done by abusing MAC prority schemes. In unfairness attacks, network performance degrades because medium access control layer priority is generally disrupted accroding to the application requirements.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<stac:occursInLayer rdf:resource="LinkLayer"/>
		<stac:hasSecurityMechanism rdf:resource="SmallFrame"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="Exhaustion">		
		<rdfs:label xml:lang="en">Battery exhaustion</rdfs:label>
		<rdfs:comment xml:lang="en">Exhaustion of the node's battery due to repeated retransmission. In exhaustion attacks the battery source is self-sacrificing, since wireless nodes most of the time maintains the channel active.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<stac:occursInLayer rdf:resource="LinkLayer"/>
		<stac:hasSecurityMechanism rdf:resource="TimeDivisionMultiplexing"/>
		<stac:hasSecurityMechanism rdf:resource="RateLimitation"/>
	</stac:SensorAttack>
	
	<!-- Attacks in the Network Layer -->
	
	<stac:SensorAttack rdf:about="Spoofed">		
		<rdfs:label xml:lang="en">Spoofed</rdfs:label>
		<rdfs:comment xml:lang="en">Routing information exchanged between sensor nodes is spoofed by an attacker.</rdfs:comment>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<stac:threatens rdf:resource="Authentication"/>	
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="Altered">		
		<rdfs:label xml:lang="en">Altered</rdfs:label>
		<rdfs:comment xml:lang="en">Routing information exchanged between sensor nodes is altered by an attacker.</rdfs:comment>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="SelectiveForwarding">		
		<rdfs:label xml:lang="en">Selective Forwarding</rdfs:label>
		<rdfs:comment xml:lang="en">Malicious nodes may ensure that certain messages are not transmitted by simply forwarding a few packets and dropping the remaining one.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<rdf:type rdf:resource="&stac;M2MAttack"/>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<rdf:type rdf:resource="&stac;MeshAttack"/>
		<stac:hasSecurityMechanism rdf:resource="MultiPathRouting"/>	
		<stac:hasSecurityMechanism rdf:resource="LLSP"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="Sybil">		
		<rdfs:label xml:lang="en">Sybil</rdfs:label>
		<rdfs:comment xml:lang="en">The malicious node presents multiple fake identities to other nodes in the network.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<rdf:type rdf:resource="&stac;M2MAttack"/>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<rdf:type rdf:resource="&stac;MeshAttack"/>
		<stac:hasSecurityMechanism rdf:resource="SPINS"/>
		<stac:hasSecurityMechanism rdf:resource="RadioResourceTesting"/>	
		<stac:hasSecurityMechanism rdf:resource="RandomKeyPredistribution"/>	
		<stac:hasSecurityMechanism rdf:resource="Registration"/>	
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="Wormhole">		
		<rdfs:label xml:lang="en">Wormhole</rdfs:label>
		<rdfs:comment xml:lang="en">An attacker tunnels messages from one location to another location. The attacker then replays those messages from the latter location.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<rdf:type rdf:resource="&stac;M2MAttack"/>
		<rdf:type rdf:resource="&stac;MeshAttack"/>
		<rdf:type rdf:resource="&stac;ManetAttack"/>
		<stac:hasSecurityMechanism rdf:resource="PacketLeaches"/>
		<stac:hasSecurityMechanism rdf:resource="Timestamp"/>
		<stac:hasSecurityMechanism rdf:resource="DirectionalAntenna"/>	
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="Sinkhole">		
		<rdfs:label xml:lang="en">Sinkhole</rdfs:label>
		<rdfs:comment xml:lang="en">An attacker tries to attract all neighboring nodes to establish routes though a malicious node. Once the attacker is successfuk in the sinkhole attack, then the network, is also open to other attacks, for example, eavesdropping or selective forwarding. Sinkhole attacks are very hard to detect.</rdfs:comment>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<rdf:type rdf:resource="&stac;M2MAttack"/>
		<rdf:type rdf:resource="&stac;MeshAttack"/>
		<stac:hasSecurityMechanism rdf:resource="Watchdog"/>
		<stac:hasSecurityMechanism rdf:resource="LLSP"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="BlackHole">		
		<rdfs:label xml:lang="en">BlackHole</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<rdf:type rdf:resource="&stac;ManetAttack"/>
		<owl:sameAs rdf:resource="Sinkhole"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="HelloFlood">		
		<rdfs:label xml:lang="en">Hello Flood</rdfs:label>
		<rdfs:comment xml:lang="en">The Hello Flood attack uses HELLO packets as a weapon to convince the sensors in Wireless Sensor Networks.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015], Paper: Routing security in sensor networks: Hello Flood Attack and Defense [Hamid et al. 2006]</dc:description>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<rdf:type rdf:resource="&stac;ManetAttack"/>
		<stac:hasSecurityMechanism rdf:resource="IdentityVerification"/>
	</stac:SensorAttack>
	
	<stac:SensorAttack rdf:about="AcknowledgmentSpoofing">		
		<rdfs:label xml:lang="en">Acknowledgment Spoofing</rdfs:label>
		<rdfs:comment xml:lang="en">An adversary spoofs link layer acknowledgment (ACK) packets of neighboring nodes. By spoofing such packets, an adversary can convince the sender that "a weak link is strong or that a dead or disabled node is alive."</rdfs:comment>
		<dc:description xml:lang="en"></dc:description>
		<stac:occursInLayer rdf:resource="NetworkLayer"/>
		<stac:threatens rdf:resource="Authentication"/>
	</stac:SensorAttack>
	
		<!-- Attacks in the Transport Layer -->
	
	<!-- Flooding -->
	
	
	<stac:SensorAttack rdf:about="Desynchronization">		
		<rdfs:label xml:lang="en">Desynchronization</rdfs:label>
		<rdfs:comment xml:lang="en">In de-synchronized attack, attacker may disrupts the established link between two legitimate two ends nodes by re-synchronizing their transmission.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description>
		<stac:occursInLayer rdf:resource="TransportLayer"/>
		<stac:hasSecurityMechanism rdf:resource="ClientPuzzle"/>
		<stac:threatens rdf:resource="Authentication"/>
	</stac:SensorAttack>

