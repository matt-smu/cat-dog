	<stac:Attack rdf:about="CiphertextOnly">		
		<rdfs:label xml:lang="en">Ciphertext Only</rdfs:label>
		<rdfs:comment xml:lang="en">The attacker only knows the ciphertext.</rdfs:comment>
	</stac:Attack>
	
	<stac:Attack rdf:about="KnownPlaintext">		
		<rdfs:label xml:lang="en">Known Plaintext</rdfs:label>
		<rdfs:comment xml:lang="en">The attacker knows pairs of plaintext and ciphertext.</rdfs:comment>
	</stac:Attack>
	
	<stac:Attack rdf:about="ChosenPlaintext">		
		<rdfs:label xml:lang="en">Chosen Plaintext</rdfs:label>
		<rdfs:comment xml:lang="en">The attacker can get ciphers of chosen plaintexts but only before the plaintext to attack (called challenge).</rdfs:comment>
	</stac:Attack>
	
	<stac:Attack rdf:about="AdaptiveChosenPlaintext">		
		<rdfs:label xml:lang="en">Adaptive Chosen Plaintext</rdfs:label>
		<rdfs:comment xml:lang="en">The attacker can gets ciphers of chosen plaintexts before and after challenge.</rdfs:comment>
	</stac:Attack>
	
	<stac:Attack rdf:about="AdaptiveChosenPlaintextCiphertext">		
		<rdfs:label xml:lang="en">Adaptive Chosen Plaintext Ciphertext</rdfs:label>
		<rdfs:comment xml:lang="en">The attacker can decipher chosen ciphertexts before and after the challenge (apart fro; this letter).</rdfs:comment>
	</stac:Attack>
	
	<stac:Attack rdf:about="ChosenCiphertext">		
		<rdfs:label xml:lang="en">Chosen Ciphertext</rdfs:label>
		<rdfs:comment xml:lang="en">The attacker can decipher chosen ciphertexts before the challenge.</rdfs:comment>
	</stac:Attack>
	
	<stac:Attack rdf:about="SideChannel">		
		<rdfs:label xml:lang="en">Side Channel</rdfs:label>
		<rdfs:comment xml:lang="en">Side channel attack is any attack based on information gained from the physical implementation of a cryptosystem.</rdfs:comment>
	</stac:Attack>		
	
