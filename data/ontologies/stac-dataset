	<!-- Key Management-->
	
	<stac:SensorKeyManagement rdf:about="LEAP_sensor">		
		<rdfs:label xml:lang="en">Localized encryption and authentication protocol (LEAP)</rdfs:label>
		<rdfs:comment xml:lang="en">LEAP is a famous key management used in sensor networks. </rdfs:comment>
		<dc:description xml:lang="en">See Paper: LEAP: Efficient security mechanisms for large-scale distributed sensor networks [Zhu et al. 2004]</dc:description>
		<stac:satisfies rdf:resource="Authentication"/>
		<stac:satisfies rdf:resource="Confidentiality"/>
		<dcterms:hasPart rdf:resource="IndividualKey"/>
		<dcterms:hasPart rdf:resource="PairwiseKey"/>
		<dcterms:hasPart rdf:resource="ClusterKey"/>
		<dcterms:hasPart rdf:resource="GroupKey"/>
	</stac:SensorKeyManagement>
	
	<stac:SymmetricSensorKey rdf:about="IndividualKey">		
		<rdfs:label xml:lang="en">Individual Key</rdfs:label>
		<rdfs:comment xml:lang="en">Every node has a unique key that it shares pairwise with the base station. This key is used for secure communication between a node and the base station. Similarly, the base station can use this key to encrypt any sensitive information.</rdfs:comment>
	</stac:SymmetricSensorKey>
	
	<stac:SymmetricSensorKey rdf:about="PairwiseKey">		
		<rdfs:label xml:lang="en">Pairwise Key</rdfs:label>
		<rdfs:comment xml:lang="en">Every node shares a pairwise key with each of its immediate neighbors. IN LEAP, pairwise keys are used for securing communications that require privacy or source authentication. In many sensor networks, the immediate neighbors of a sensor node cannot be predicted in advance; consequently, these pairwise shared keys will need to be established after the network is deployed.</rdfs:comment>
	</stac:SymmetricSensorKey>
	
	<stac:SymmetricSensorKey rdf:about="ClusterKey">		
		<rdfs:label xml:lang="en">Cluster Key</rdfs:label>
		<rdfs:comment xml:lang="en">A cluster key is a key shared by a node and all its neighbors, and it is mainly used for securing locally broadcast messages, e.g., routing control information, or securing sensor messages which can benefit from passive participation.</rdfs:comment>
	</stac:SymmetricSensorKey>
	
	<stac:SymmetricSensorKey rdf:about="GroupKey">		
		<rdfs:label xml:lang="en">Group Key</rdfs:label>
		<rdfs:comment xml:lang="en">This is globally shared key that is used by the base station for encrypting messages that are broadcast to the whole group.</rdfs:comment>
	</stac:SymmetricSensorKey>
	
	<!-- SecurityMechanisms in the Physical Layer -->
	
	<!-- SecurityMechanisms in the Link Layer -->
	
	<stac:SensorSecurityMechanism rdf:about="SmallFrame">		
		<rdfs:label xml:lang="en">Small Frame</rdfs:label>
		<rdfs:comment xml:lang="en">Small frames are used so that an individual node can capture the channel only for a short time.</rdfs:comment>
		<stac:protectsInLayer rdf:resource="LinkLayer"/>
	</stac:SensorSecurityMechanism>
	
	<stac:SensorSecurityMechanism rdf:about="ErrorCorrectingCode">		
		<rdfs:label xml:lang="en">Error Correcting Code</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
